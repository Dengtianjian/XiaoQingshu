/* components/navigationBar/navigationBar.wxss */
.navigation-bar {
  position: fixed;
  z-index:30;
  top: 0;
  left: 0;
  display: block;
  width: 100%;
  background:transparent;
}
.navigation-bar-background{
  position:fixed;
  z-index:1;
  top:0;
  left:0;
  width:100%;
}
.navigator-bar-safe-area {
  position:relative;
  z-index:30;
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 100%;
  padding: 0 var(--container-padding);
  box-sizing: border-box;
  transition:color 1s lienar;
  /* background:red; */
}