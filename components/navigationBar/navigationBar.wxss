/* components/navigationBar/navigationBar.wxss */
.navigation-bar {
  position: fixed;
  top: 0;
  left: 0;
  display: block;
  width: 100%;
  background:transparent;
}
.navigation-bar-background{
  background:black;
}
.navigator-bar-safe-area {
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 100%;
  padding: 0 var(--container-padding);
  box-sizing: border-box;
  /* background:red; */
}